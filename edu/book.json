{
  "gitbook": "^3.2.2",
  "plugins": [
    "edit-link",
    "localized-footer",
    "scramble-mailto",
    "hide-published-with",
    "youtubex",
    "katex",
    "collapsible-chapters",
    "sectionx@git+https://github.com/noerw/gitbook-plugin-sectionx.git#v3.2.0",
    "sitemap-general"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/sensebox/books/tree/master/edu",
      "label": {
        "en": "Edit This Page",
        "de": "Seite bearbeiten"
      }
    },
    "sitemap-general": {
      "prefix": "https://books.sensebox.de/edu/"
    },
    "sectionx": {
      "tag": "b"
    },
    "theme-default": {
      "showLevel": true
    }
  }
}
